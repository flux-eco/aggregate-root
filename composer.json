  "license": "GPL-3.0-only",
  "authors": [
    {
      "name": "fluxlabs ag",
      "email": "support@fluxlabs.ch",
      "homepage": "https://fluxlabs.ch",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@fluxlabs.ch"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/global-stream.git"
    },
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/storage.git"
