    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/value-object.git"
    },
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/json-schema-assertion.git"
    },
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/json-schema-instance.git"
    },
    {
      "type": "vcs",
      "url": "ssh://git@git.fluxlabs.ch:10022/fluxlabs/flux-eco/json-schema-document.git"
    }
  ],
  "require": {
    "flux-eco/global-stream": "dev-main",
    "flux-eco/storage": "dev-main",
    "flux-eco/value-object": "dev-main",
    "flux-eco/json-schema-assertion": "dev-main",
    "flux-eco/json-schema-instance": "dev-main",
    "flux-eco/json-schema-document": "dev-main",
    "php": ">=8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-yaml": "*"
  },
  "require-dev": {

  },
  "autoload": {
    "psr-4": {
      "FluxEco\\AggregateRoot\\": [
        "src/"
      ]
    }
